  "description": "ISC Broker PWA",
  "main": "index.js",
  "scripts": {
    "dev-server": "vite --open",
    "dev": "npm run dev-server",
    "dev-task": "vite",
    "prebuild": "npx workbox-cli copyLibraries public/",
