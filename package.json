  "author": "",
  "license": "ISC",
  "dependencies": {
    "esbuild": "^0.25.0",
    "got": "^11.8.5",
    "braces": "^3.0.3",
    "@shoelace-style/shoelace": "^2.20.0",
    "@thepassle/app-tools": "^0.9.12",
    "bcryptjs": "^3.0.0",
    "lit": "^3.2.1",
    "urlpattern-polyfill": "^10.0.0",
    "workbox-build": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-sw": "^7.3.0"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "gh-pages": "^6.3.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-pwa": "^0.21.1",
    "workbox-cli": "^7.3.0"
  },
  "prettier": {
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
